{
    "name": "Baïkal",
    "id": "baikal",
    "packaging_format": 1,
    "description": {
        "en": "Lightweight CalDAV and CardDAV server",
        "fr": "Serveur CalDAV et CardDAV léger"
    },
    "version": "0.9.3~ynh1",
    "url": "http://baikal-server.com/",
    "upstream": {
        "license": "GPL-3.0",
        "website": "http://sabre.io/baikal/",
        "demo": "https://demo.yunohost.org/baikal/admin/",
        "admindoc": "https://sabre.io/dav/",
        "userdoc": "https://github.com/AlexandreMonroche/BaikalGuide",
        "code": "https://github.com/sabre-io/Baikal"
    },
    "license": "GPL-3.0",
    "maintainer": {
        "name": "",
        "email": ""
    },
    "requirements": {
        "yunohost": ">= 11.0.9"
    },
    "multi_instance": false,
    "services": [
        "nginx",
        "php8.0-fpm",
        "mysql"
    ],
    "arguments": {
        "install": [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/baikal",
                "default": "/baikal"
            },
            {
                "name": "password",
                "type": "password"
            }
        ]
    }
}

{
    "id": "baikal",
    "name": "Baikal",
    "packaging_format": 1,
    "description": {
        "en": "Lightweight CalDAV and CardDAV server",
        "fr": "Serveur CalDAV et CardDAV lÃ©ger"
    },
    "version": "0.7.1~ynh2",
    "url": "http://baikal-server.com/",
    "license": "GPL-3.0",
    "maintainer": {
        "name": "julien",
        "email": "julien.malik@paraiso.me"
    },
    "requirements": {
        "yunohost": ">= 4.1.7"
    },
    "multi_instance": false,
    "services": [
        "nginx",
        "php7.3-fpm",
        "mysql"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain",
                "example": "domain.org"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/baikal",
                "default": "/baikal"
            },
            {
                "name": "password",
                "type": "password",
                "example": "mysecret"
            }
        ]
    }
}
